<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qs.erp.daos.dao.CartoontypesDao">

    <insert id="Save">
        INSERT  INTO  cartoontypes (RowId,CartoonRowId,CartoonTypeRowId)
        VALUES (
        #{RowId},
        #{CartoonRowId},
        #{CartoonTypeRowId}
        )
    </insert>

    <delete id="Delete">
        DELETE FROM cartoontypes WHERE  CartoonRowId=#{CartoonRowId}
    </delete>

</mapper>